{
  "hosting": {
    "site": "projetos-angular-fabi",
    "public": "dist/movie-instagram",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "rules": {
    "some_path/$uid": {
      ".read": true,
      ".write": "auth.uid === $uid"
    }
  }
}



